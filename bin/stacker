#!/usr/bin/env coffee
require('es6-promise').polyfill()
require 'colors'
_ = require 'lodash'
stacker = require '../lib/stacker_lib'
require('climet-client')()

process.on 'uncaughtException', (error) ->
  console.log 'unhandled exception!'.red
  console.log error
  console.log error.stack

# DO IT!
stacker.boot()
